{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": ["**/.env.*local"],
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["build/**", ".next/**"]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "publish": {
            "cache": false
        }
    }
}
